<script>
    $.ajax({method:"GET", url:"./getIngredients"})
    .done((res)=>{
        console.log(res.data);
        $("#ingredients").html(toTable(res.data.ingredients, false, ["ID", "Név", "Kcal", "Fehérje", "Szénhidrát", "Zsír", "Ehető-e magában"], "ingredientsTable"));
        tableItemEdit("ingredientsTable", "ehetoe_magaban", "1", "'Igen'", "'Nem'");
    })
    .fail((err)=>{

    })
    .always(()=>{})
</script>

