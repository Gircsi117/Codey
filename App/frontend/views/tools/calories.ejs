<%- include("../header") %>
<h1 style="color: red; text-align: center" class="error">Az oldal nem támogatja ezt a képernyő szélességet</h1>
<div class="container mt-2 mt-lg-3 top-p">
    <%- include("../menu") %>
    <div class="content row m-0 p-3">
        <div class="content-wrapper p-3" id="ingredients" style="overflow-x: scroll;">

        </div>
    </div>
</div>
<%- include("../foot") %>

<script>
    $.ajax({method:"GET", url:"./getIngredients"})
    .done((res)=>{
        console.log(res.data);
        $("#ingredients").html(toTable({
            datas:res.data.ingredients,
            headers: ["ID", "Név", "Kcal", "Fehérje", "Szénhidrát", "Zsír", "Ehető-e magában"], 
            tableID:"ingredientsTable"
        }));
        tableItemEdit({table:"ingredientsTable", item:"ehetoe_magaban",  param:"1", result1:"'Igen'", result2:"'Nem'"});
    })
    .fail((err)=>{

    })
    .always(()=>{})
</script>

<%- include("../footer") %>
